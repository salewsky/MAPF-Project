#!/bin/bash
#SBATCH --output=out.%j
#SBATCH --error=err.%j
#SBATCH --time=03:10:00        # walltime
#SBATCH --cpus-per-task=4    # number of processor cores (i.e. tasks)
#SBATCH --partition=long

# Good Idea to stop operation on first error.
set -e

# Load environment modules for your application here.
source /etc/profile.d/modules.sh
module load anaconda3
source activate Clingo2
# Run jobs
jobs="results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run1/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run2/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run3/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run4/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run5/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run6/start.sh
results/Random/empty-1-setting-1/Random/large-open-128-128.lp/run7/start.sh
"
echo running jobs@$(hostname)...
echo ""
echo "jobs:" $jobs
echo ""
for i in $jobs
do
    echo "running " $i
    $i
done
